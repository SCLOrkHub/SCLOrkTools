namespace Confab.Data;

// Keep these the same as the enum in Confab::Asset
enum Type : uint {
    kInvalid = 1,
    kSnippet = 2,
    kImage = 3,
    kYAML = 4,
    kSample = 5
}

table FlatAsset {
    type:Type = kInvalid;

    key:ulong;
    name:string;
    fileExtension:string;
    author:ulong;
    deprecatedBy:ulong;
    deprecates:ulong;
    inlineData:[ubyte];
    expiresOn:ulong;
    salt:ulong;
}

root_type FlatAsset;

