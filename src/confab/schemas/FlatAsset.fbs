namespace Confab.Data;

enum Type : uint {
    kInvalid = 1,
    kSnippet,
    kImage,
    kYAML,
    kSample
}

table FlatAsset {
    type:Type = kInvalid;

    key:ulong;
    name:string;
    fileExtension:string;
    author:ulong;
    deprecatedBy:ulong;
    deprecates:[ulong];
    inlineData:[ubyte];
    expiresOn:ulong;
}

root_type FlatAsset;

